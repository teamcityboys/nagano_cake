<div class="container">
  <div class="admins-orders pt-5">
    <h4 class="pb-3">注文履歴一覧</h4>
    <table class="table">
      <tr class="table-info.bg-info">
       <th>購入日時</th>
       <th>購入者</th>
       <th>注文個数</th>
       <th>注文ステータス</th>
      </tr>
      <% @orders.each do |order| %>
        <tr>
          <td>
            <%= link_to admins_order_path(order), class: "text-dark" %>
            <%= @order.created_at.strftime("%Y/%m/%d") %>
          </td>
          <tb><%= order.customer.full_name %></tb>
          <tb><%= order.order_details.quantity %></tb>
          <tb><%= order.order_status %></tb>
        </tr>
      <% end %>  
    </table>
  </div>
</div>
   